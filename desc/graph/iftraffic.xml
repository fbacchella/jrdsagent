<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
  <name>IfTraffic</name>
  <graphName>iftraffic-{2}</graphName>
  <graphTitle>Interface traffic on interface {2} on {1}</graphTitle>
  <verticalLabel>b/s</verticalLabel>
  <lowerLimit>NaN</lowerLimit>
  <add>
    <legend>Received (upward graph)</legend>
  </add>
  <add>
    <name>txbits</name>
    <graphType>line</graphType>
    <legend>Bits transmitted</legend>
  </add>
  <add>
    <legend>Transmit (downward graph)</legend>
  </add>
  <add>
    <name>rxbits</name>
    <reversed>true</reversed>
    <graphType>line</graphType>
    <color>green</color>
    <legend>Bits received</legend>
  </add>
  <hosttree>
    <pathelement>HOST</pathelement>
    <pathelement>NETWORK</pathelement>
    <pathelement>INTERFACES</pathelement>
    <pathelement>INDEX</pathelement>
    <pathstring>Interface traffic</pathstring>
  </hosttree>
  <viewtree>
    <pathelement>NETWORK</pathelement>
    <pathelement>INTERFACES</pathelement>
    <pathstring>Interface traffic</pathstring>
    <pathelement>HOST</pathelement>
    <pathelement>INDEX</pathelement>
  </viewtree>
</graphdesc>
