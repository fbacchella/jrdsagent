<?xml version="1.0" encoding="ISO-8859-1"?>
	<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>CpuStatsVirtualLinux</name>
    <probeName>cpustats</probeName>
    <probeClass>jrds.probe.MultiNoKeys</probeClass>
    <specific name="statFile">/proc/stat</specific>
    <ds>
        <dsName>user</dsName>
        <dsType>counter</dsType>
        <collect>cpu.1</collect>
    </ds>
    <ds>
        <dsName>nice</dsName>
        <dsType>counter</dsType>
        <collect>cpu.2</collect>
    </ds>
    <ds>
        <dsName>system</dsName>
        <dsType>counter</dsType>
        <collect>cpu.3</collect>
    </ds>
    <ds>
        <dsName>idle</dsName>
        <dsType>counter</dsType>
        <collect>cpu.4</collect>
    </ds>
    <ds>
        <dsName>iowait</dsName>
        <dsType>counter</dsType>
        <collect>cpu.5</collect>
    </ds>
    <ds>
        <dsName>irq</dsName>
        <dsType>counter</dsType>
        <collect>cpu.6</collect>
    </ds>
    <ds>
        <dsName>softirq</dsName>
        <dsType>counter</dsType>
        <collect>cpu.7</collect>
    </ds>
    <ds>
        <dsName>steal_time</dsName>
        <dsType>counter</dsType>
        <collect>cpu.8</collect>
        <defaultValue>0</defaultValue>
    </ds>
    <ds>
        <dsName>guest</dsName>
        <dsType>counter</dsType>
        <collect>cpu.9</collect>
        <defaultValue>0</defaultValue>
    </ds>
    <ds>
        <dsName>interrupts</dsName>
        <dsType>counter</dsType>
        <collect>intr.1</collect>
    </ds>
    <ds>
        <dsName>ctxt</dsName>
        <dsType>counter</dsType>
        <collect>ctxt.1</collect>
    </ds>
    <ds>
        <dsName>processes</dsName>
        <dsType>counter</dsType>
        <collect>processes.1</collect>
    </ds>
    <ds>
        <dsName>procs_running</dsName>
        <dsType>gauge</dsType>
        <collect>procs_running.1</collect>
    </ds>
    <ds>
        <dsName>procs_blocked</dsName>
        <dsType>gauge</dsType>
        <collect>procs_blocked.1</collect>
    </ds>
    <graphs>
        <name>CPUTimeStatLinux</name>
        <name>VirtualCPUTimeStatLinux</name>
    </graphs>
</probedesc>
