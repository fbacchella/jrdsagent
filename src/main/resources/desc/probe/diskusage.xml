<probedesc>
    <name>DiskUsage</name>
    <probeName>du-${index.cleanpath}</probeName>
    <probeClass>jrds.probe.RMIIndexed</probeClass>
    <specific name="remote">jrds.agent.DiskUsage</specific>
    <ds>
        <dsName>total</dsName>
        <dsType>gauge</dsType>
    </ds>
    <ds>
        <dsName>usable</dsName>
        <dsType>gauge</dsType>
    </ds>
    <ds>
        <dsName>free</dsName>
        <dsType>gauge</dsType>
    </ds>
    <graphs>
        <name>DiskUsage</name>
    </graphs>
</probedesc>