<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>PerfOS_Processor</name>
    <probeName>processor${index}</probeName>
    <probeClass>jrds.probe.WmiProbeIndexed</probeClass>
    <specific name="wbemClass">Win32_PerfRawData_PerfOS_Processor</specific>
    <specific name="key">Name</specific>
    <defaultargs>
        <attr name="index">_Total</attr>
    </defaultargs>
    <ds>
        <dsName>C1Time</dsName>
        <dsType>counter</dsType>
        <collect>PercentC1Time</collect>
    </ds>
    <ds>
        <dsName>C2Time</dsName>
        <dsType>counter</dsType>
        <collect>PercentC2Time</collect>
    </ds>
    <ds>
        <dsName>C3Time</dsName>
        <dsType>counter</dsType>
        <collect>PercentC3Time</collect>
    </ds>
    <ds>
        <dsName>DPCTime</dsName>
        <dsType>counter</dsType>
        <collect>PercentC3Time</collect>
    </ds>
    <ds>
        <dsName>IdleTime</dsName>
        <dsType>counter</dsType>
        <collect>PercentIdleTime</collect>
    </ds>
    <ds>
        <dsName>InterruptTime</dsName>
        <dsType>counter</dsType>
        <collect>PercentInterruptTime</collect>
    </ds>
    <ds>
        <dsName>PrivilegedTime</dsName>
        <dsType>counter</dsType>
        <collect>PercentPrivilegedTime</collect>
    </ds>
    <ds>
        <dsName>ProcessorTime</dsName>
        <dsType>counter</dsType>
        <collect>PercentProcessorTime</collect>
    </ds>
    <ds>
        <dsName>UserTime</dsName>
        <dsType>counter</dsType>
        <collect>PercentUserTime</collect>
    </ds>
    <graphs>
        <name>PerfOS_ProcessorTime</name>
    </graphs>
</probedesc>
