<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>ProcessInfoCpu</name>
    <probeName>picpu-${index}</probeName>
    <probeClass>jrds.probe.ProcessInfo</probeClass>
    <specific name="remote">jrds.agent.linux.ProcessCpu</specific>
    <ds>
        <dsName>count</dsName>
        <dsType>gauge</dsType>
        <collect>count</collect>
    </ds>
    <ds>
        <dsName>maj_flt</dsName>
        <dsType>counter</dsType>
        <collect>stat:maj_flt</collect>
    </ds>
    <ds>
        <dsName>cmaj_flt</dsName>
        <dsType>counter</dsType>
        <collect>stat:cmaj_flt</collect>
    </ds>
    <ds>
        <dsName>utime</dsName>
        <dsType>counter</dsType>
        <collect>stat:task_times.tms_utime</collect>
    </ds>
    <ds>
        <dsName>stime</dsName>
        <dsType>counter</dsType>
        <collect>stat:task_times.tms_stime</collect>
    </ds>
    <ds>
        <dsName>gtime</dsName>
        <dsType>counter</dsType>
        <collect>stat:gtime</collect>
    </ds>
    <ds>
        <dsName>cutime</dsName>
        <dsType>counter</dsType>
        <collect>stat:task_times.tms_cutime</collect>
    </ds>
    <ds>
        <dsName>cstime</dsName>
        <dsType>counter</dsType>
        <collect>stat:task_times.tms_cstime</collect>
    </ds>
    <ds>
        <dsName>cgtime</dsName>
        <dsType>counter</dsType>
        <collect>stat:cgtime</collect>
    </ds>
    <ds>
        <dsName>biotime</dsName>
        <dsType>counter</dsType>
        <collect>stat:blkio_ticks</collect>
    </ds>
    <ds>
        <dsName>num_threads</dsName>
        <dsType>gauge</dsType>
        <collect>stat:num_threads</collect>
    </ds>
    <graphs>
        <name>ProcessInfo-threads</name>
        <name>ProcessInfo-maj_faults</name>
        <name>ProcessInfo-time</name>
    </graphs>
</probedesc>
