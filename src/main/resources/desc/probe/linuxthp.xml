<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>THPLinux</name>
    <probeName>thp</probeName>
    <probeClass>jrds.probe.MultiNoKeys</probeClass>
    <specific name="statFile">/proc/vmstat</specific>
    <ds>
        <dsName>fault_alloc</dsName>
        <dsType>counter</dsType>
        <collect>thp_fault_alloc.1</collect>
    </ds>
    <ds>
        <dsName>collapse_alloc</dsName>
        <dsType>counter</dsType>
        <collect>thp_collapse_alloc.1</collect>
    </ds>
    <ds>
        <dsName>fault_fallback</dsName>
        <dsType>counter</dsType>
        <collect>thp_fault_fallback.1</collect>
    </ds>
    <ds>
        <dsName>cllps_alloc_failed</dsName>
        <dsType>counter</dsType>
        <collect>thp_collapse_alloc_failed.1</collect>
    </ds>
    <ds>
        <dsName>split</dsName>
        <dsType>counter</dsType>
        <collect>thp_split.1</collect>
    </ds>
    <ds>
        <dsName>zero_page_alloc</dsName>
        <dsType>counter</dsType>
        <collect>zero_page_alloc.1</collect>
    </ds>
    <ds>
        <dsName>zr_page_alloc_failed</dsName>
        <dsType>counter</dsType>
        <collect>thp_zero_page_alloc_failed.1</collect>
    </ds>
    <ds>
        <dsName>compact_stall</dsName>
        <dsType>counter</dsType>
        <collect>compact_stall.1</collect>
    </ds>
    <ds>
        <dsName>compact_success</dsName>
        <dsType>counter</dsType>
        <collect>compact_success.1</collect>
    </ds>
    <ds>
        <dsName>compact_fail</dsName>
        <dsType>counter</dsType>
        <collect>compact_fail.1</collect>
    </ds>
    <ds>
        <dsName>compact_pages_moved</dsName>
        <dsType>counter</dsType>
        <collect>compact_pages_moved.1</collect>
    </ds>
    <ds>
        <dsName>cmpct_pgmgrt_failed</dsName>
        <dsType>counter</dsType>
        <collect>compact_pagemigrate_failed.1</collect>
    </ds>
    <ds>
        <dsName>compact_blocks_moved</dsName>
        <dsType>counter</dsType>
        <collect>compact_blocks_moved.1</collect>
    </ds>
    <graphs>
        <name>LinuxTHPCompact</name>
        <name>LinuxTHPMoved</name>
    </graphs>
</probedesc>
