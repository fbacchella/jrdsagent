<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
    <name>NodeMemoryLinux</name>
    <graphName>nodemem</graphName>
    <graphClass>jrds.probe.NodeMemoryGraph</graphClass>
    <graphTitle>Node Memory on ${host}</graphTitle>
    <unit>
        <binary />
    </unit>
    <verticalLabel>Bytes</verticalLabel>
    <hosttree>
        <pathstring>${host}</pathstring>
        <pathelement>SYSTEM</pathelement>
        <pathelement>MEMORY</pathelement>
        <pathstring>Node memory usage</pathstring>
    </hosttree>
    <viewtree>
        <pathelement>SYSTEM</pathelement>
        <pathelement>MEMORY</pathelement>
        <pathstring>Node memory usage</pathstring>
        <pathstring>${host}</pathstring>
    </viewtree>
</graphdesc>
