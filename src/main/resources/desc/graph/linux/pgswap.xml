<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
    <name>pgswap</name>
    <graphName>pgswap</graphName>
    <graphTitle>Swapping activity on ${host}</graphTitle>
    <verticalLabel>Operation/s</verticalLabel>
    <add>
        <name>pswpout</name>
        <graphType>line</graphType>
        <color>blue</color>
        <legend>Number of pages swapped out</legend>
    </add>
    <add>
        <name>pswpin</name>
        <reversed>true</reversed>
        <graphType>line</graphType>
        <color>green</color>
        <legend>Number of pages swapped in</legend>
    </add>
    <hosttree>
        <pathelement>HOST</pathelement>
        <pathelement>SYSTEM</pathelement>
        <pathelement>MEMORY</pathelement>
        <pathstring>VM activity details</pathstring>
        <pathstring>Swap activity</pathstring>
    </hosttree>
    <viewtree>
        <pathelement>SYSTEM</pathelement>
        <pathelement>MEMORY</pathelement>
        <pathstring>VM activity details</pathstring>
        <pathstring>Swap activity</pathstring>
        <pathelement>HOST</pathelement>
    </viewtree>
</graphdesc>
