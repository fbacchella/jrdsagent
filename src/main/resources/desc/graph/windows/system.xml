<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
    <name>threads</name>
    <graphName>threads</graphName>
    <graphTitle>Number of threads and process on ${host}</graphTitle>
    <verticalLabel>Number</verticalLabel>
    <add>
        <name>Processes</name>
        <graphType>line</graphType>
        <color>green</color>
        <legend>Number of processes</legend>
    </add>
    <add>
        <name>Threads</name>
        <graphType>line</graphType>
        <color>blue</color>
        <legend>Number of threads</legend>
    </add>
    <hosttree>
        <pathelement>HOST</pathelement>
        <pathelement>SYSTEM</pathelement>
        <pathelement>LOAD</pathelement>
        <pathstring>Number of threads</pathstring>
    </hosttree>
    <viewtree>
        <pathelement>SYSTEM</pathelement>
        <pathelement>LOAD</pathelement>
        <pathstring>Number of threads</pathstring>
        <pathelement>HOST</pathelement>
    </viewtree>
</graphdesc>