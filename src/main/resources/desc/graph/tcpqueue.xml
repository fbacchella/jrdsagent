<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
  <name>TcpQueue</name>
  <graphName>tcpq-{2}</graphName>
  <graphTitle>Average queue length for port {2} on host {1}</graphTitle>
  <verticalLabel>bytes</verticalLabel>
  <add>
    <name>rx</name>
    <graphType>line</graphType>
    <color>green</color>
    <legend>Received queue length</legend>
  </add>
  <add>
    <name>tx</name>
    <graphType>line</graphType>
    <color>blue</color>
    <legend>Transmit queue length</legend>
  </add>
  <hosttree>
    <pathelement>HOST</pathelement>
    <pathelement>NETWORK</pathelement>
    <pathelement>TCP</pathelement>
    <pathstring>Queue length</pathstring>
    <pathelement>INDEX</pathelement>
  </hosttree>
  <viewtree>
    <pathelement>NETWORK</pathelement>
    <pathelement>TCP</pathelement>
    <pathstring>Queue length</pathstring>
    <pathelement>HOST</pathelement>
    <pathelement>INDEX</pathelement>
  </viewtree>
</graphdesc>
